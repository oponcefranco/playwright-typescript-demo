{
  "name": "playwright-typescript-demo",
  "version": "1.0.0",
  "description": "Playwright Typescript Demo",
  "type": "module",
  "main": "index.js",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "start": "npx ts-node index.ts",
    "test": "npx playwright test",
    "test:chromium": "npx playwright test --project=chromium",
    "test:debug": "npx playwright test --debug",
    "test:ui": "npx playwright test --ui",
    "test:report": "npx playwright show-report",
    "eslint-check-only": "npx eslint .",
    "eslint-fix": "npx eslint . --fix",
    "prettier": "npx prettier --write ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oponcefranco/playwright-typescript-demo.git"
  },
  "keywords": [
    "playwright-demo",
    "automation"
  ],
  "author": "Oleg Poncefranco",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/oponcefranco/playwright-typescript-demo/issues"
  },
  "homepage": "https://github.com/oponcefranco/playwright-typescript-demo#readme",
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@playwright/test": "^1.55.0",
    "@types/node": "^24.5.2",
    "@typescript-eslint/eslint-plugin": "^8.44.0",
    "@typescript-eslint/parser": "^8.44.0",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^16.4.0",
    "jiti": "^2.5.1",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.44.0"
  },
  "dependencies": {
    "dotenv": "^17.2.2"
  }
}
